version: '3'
services:
  artifactory:
    image: docker.bintray.io/jfrog/artifactory-oss:5.10.1
    network_mode: host
    restart: always
    ports:
      - 8081:8081
    volumes:
      - /var/data/artifactory:/var/opt/jfrog/artifactory
    environment:
      EXTRA_JAVA_OPTIONS: -Xmx512m
      SERVICE_NAME: system/artifactory
      SERVICE_CHECK_TCP: "true"
